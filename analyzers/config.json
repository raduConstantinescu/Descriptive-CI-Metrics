{
  "RepoGenerator": {
    "languages": ["javascript", "java", "typescript", "python"],
    "min_stars": 100,
    "min_days_old": 180,
    "output_file": "../repos.txt",
    "exclude_repos_file": "../repos.txt"
  },
  "RepoWorkflowBuildFilter": {
    "input_file": "./new_output/intermediate_results1.json",
    "output_file": "./new_output/clean_rep_data1.json",
    "keywords": ["build", "test", "CI"],
    "max_workflows": 5,
    "min_runs": 500
  },
  "JsonMergeStage": {
    "input_files": ["./new_output/clean_rep_data.json" , "./new_output/clean_rep_data1.json", "./new_output/clean_rep_data2.json"],
    "output_file": "./new_output/all_repo_data.json"
 },
  "CIAnalyzer" : {
    "input_file": "./new_output/all_repo_data.json",
    "output_file": "./new_output/ci_data.json",
    "stats_file": "./new_output/ci_stats.json"
  },
  "CIPlotter": {
    "input_file": "./new_output/ci_data.json",
    "data_input_file": "./new_output/ci_stats.json",
    "output_dir": "./new_output/plots/"
  },
  "CIQuadrantPlotter" : {
    "input_file": "./new_output/ci_data.json",
    "data_input_file": "./new_output/ci_stats.json",
    "output_dir": "./new_output/plots/"
  }
}
